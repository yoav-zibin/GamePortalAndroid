<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners
                android:topRightRadius="30dip"
                android:bottomLeftRadius="30dip"
                />
            <stroke android:width="1dip" android:color="#5e7974" />
            <gradient android:angle="315" android:startColor="#90cc8e" android:endColor="#248f24"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners
                android:topRightRadius="30dip"
                android:bottomLeftRadius="30dip"
                />
            <stroke android:width="1dip" android:color="#5e7974" />
            <solid android:color="#90cc8e"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners
                android:topRightRadius="30dip"
                android:bottomLeftRadius="30dip"
                />
            <stroke android:width="1dip" android:color="#5e7974" />
            <gradient android:angle="315" android:startColor="#99ff99" android:endColor="#90cc8e" />
        </shape>
    </item>
</selector>